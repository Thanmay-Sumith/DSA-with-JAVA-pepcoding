//Digits Frequency:-
import java.util.*;

public class Main{
    
public static void main(String[] args){
    Scanner scn = new Scanner(System.in);
    int n = scn.nextInt();
    int r = scn.nextInt();
    int Ans = DigitFrequency(n,r);
    display(Ans);
}

public static void display(int Ans){
    System.out.println(Ans);
}

public static int DigitFrequency(int n,int r){
    int rv = 0;
    
    while(n > 0){
        int dig = n % 10;
        n=n/10;
        if(dig == r){
            rv++;
        }
        
    }
    return rv;
}
}
