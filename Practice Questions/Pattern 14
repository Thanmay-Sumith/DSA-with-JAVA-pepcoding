/*Print Table of a Number in the form:-
n * 1 = n
n * 2 = 2n
.........
n * 10 = 10n
*/
import java.util.*;

public class Main{
    
public static void main(String[] args){
    Scanner scn = new Scanner(System.in);
    int n = scn.nextInt();
    
    for(int i = 1; i <= 10;i++){
        int value = n * i;
        System.out.println(n + " * " + i + " = " + value);
    }
}
}
